<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/anasayfa_nav_graph"
    app:startDestination="@id/anasayfaFragment">

    <fragment
        android:id="@+id/anasayfaFragment"
        android:name="com.bilgehankalay.gizlirehber.Fragment.AnasayfaFragment"
        android:label="fragment_anasayfa"
        tools:layout="@layout/fragment_anasayfa" >
        <action
            android:id="@+id/anasayfa_to_kisiekle"
            app:destination="@id/kisiEkleFragment" />
        <action
            android:id="@+id/anasayfa_to_kisidetay"
            app:destination="@id/kisiDetayFragment" />
    </fragment>
    <fragment
        android:id="@+id/kisiEkleFragment"
        android:name="com.bilgehankalay.gizlirehber.Fragment.KisiEkleFragment"
        android:label="fragment_kisi_ekle"
        tools:layout="@layout/fragment_kisi_ekle" >
        <action
            android:id="@+id/kisiEkle_to_anasayfa"
            app:destination="@id/anasayfaFragment"
            app:popUpTo="@id/anasayfaFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/kisiDetayFragment"
        android:name="com.bilgehankalay.gizlirehber.Fragment.KisiDetayFragment"
        android:label="fragment_kisi_detay"
        tools:layout="@layout/fragment_kisi_detay" >
        <action
            android:id="@+id/kisiDetay_to_anasayfa"
            app:destination="@id/anasayfaFragment"
            app:popUpTo="@id/anasayfaFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="kisi"
            app:argType="com.bilgehankalay.gizlirehber.Model.KisiModel" />
    </fragment>
</navigation>